-- Variables to track checkbox states and cooldowns
local freezeTradeEnabled = false
local autoAcceptEnabled = false
local freezeCooldownActive = false
local autoCooldownActive = false

-- Function to show warning popup
local function showWarning(message)
    local warningGui = Instance.new("ScreenGui")
    warningGui.Name = "WarningPopup"
    warningGui.Parent = playerGui
    
    local warningFrame = Instance.new("Frame")
    warningFrame.Size = UDim2.new(0, 300, 0, 150)
    warningFrame.Position = UDim2.new(0.5, -150, 0.5, -75)
    warningFrame.BackgroundColor3 = Color3.fromRGB(255, 100, 100)
    warningFrame.BorderSizePixel = 3
    warningFrame.BorderColor3 = Color3.fromRGB(255, 0, 0)
    warningFrame.Active = true
    warningFrame.Draggable = true
    warningFrame.Parent = warningGui
    
    local warningCorner = Instance.new("UICorner")
    warningCorner.CornerRadius = UDim.new(0, 10)
    warningCorner.Parent = warningFrame
    
    local warningTitle = Instance.new("TextLabel")
    warningTitle.Size = UDim2.new(1, 0, 0, 30)
    warningTitle.Position = UDim2.new(0, 0, 0, 5)
    warningTitle.BackgroundTransparency = 1
    warningTitle.Text = "⚠️ WARNING ⚠️"
    warningTitle.TextColor3 = Color3.fromRGB(255, 255, 255)
    warningTitle.TextScaled = true
    warningTitle.Font = Enum.Font.SourceSansBold
    warningTitle.TextXAlignment = Enum.TextXAlignment.Center
    warningTitle.Parent = warningFrame
    
    local warningMessage = Instance.new("TextLabel")
    warningMessage.Size = UDim2.new(1, -20, 0, 60)
    warningMessage.Position = UDim2.new(0, 10, 0, 40)
    warningMessage.BackgroundTransparency = 1
    warningMessage.Text = message
    warningMessage.TextColor3 = Color3.fromRGB(255, 255, 255)
    warningMessage.TextScaled = true
    warningMessage.Font = Enum.Font.SourceSans
    warningMessage.TextXAlignment = Enum.TextXAlignment.Center
    warningMessage.TextWrapped = true
    warningMessage.Parent = warningFrame
    
    local okButton = Instance.new("TextButton")
    okButton.Size = UDim2.new(0, 80, 0, 30)
    okButton.Position = UDim2.new(0.5, -40, 1, -40)
    okButton.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
    okButton.BorderSizePixel = 2
    okButton.BorderColor3 = Color3.fromRGB(200, 200, 200)
    okButton.Text = "OK"
    okButton.TextColor3 = Color3.fromRGB(0, 0, 0)
    okButton.TextScaled = true
    okButton.Font = Enum.Font.SourceSansBold
    okButton.Parent = warningFrame
    
    local okCorner = Instance.new("UICorner")
    okCorner.CornerRadius = UDim.new(0, 6)
    okCorner.Parent = okButton
    
    okButton.MouseButton1Click:Connect(function()
        warningGui:Destroy()
    end)
    
    -- Rainbow animation for warning popup
    local warningRainbowConnection
    warningRainbowConnection = RunService.Heartbeat:Connect(function()
        if not warningFrame.Parent then
            warningRainbowConnection:Disconnect()
            return
        end
        
        local time = tick() * 3
        local r = math.sin(time) * 0.3 + 0.7
        local g = math.sin(time + 2) * 0.3 + 0.3
        local b = math.sin(time + 4) * 0.3 + 0.3
        
        warningFrame.BackgroundColor3 = Color3.new(r, g, b)
    end)
end

-- Function to update checkbox appearance
local function updateCheckbox(checkbox, enabled)
    if enabled then
        checkbox.Text = "✓"
        checkbox.TextColor3 = Color3.fromRGB(0, 150, 0)
        checkbox.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
    else
        checkbox.Text = ""
        checkbox.BackgroundColor3 = Color3.fromRGB(200, 200, 200)
    end
end

-- Function to start freeze trade cooldown
local function startFreezeCooldown()
    freezeCooldownActive = true
    freezeCooldownLabel.Visible = true
    local cooldownValue = 1
    local timeRemaining = 60 -- 60 seconds
    
    local cooldownConnection
    cooldownConnection = RunService.Heartbeat:Connect(function()
        if timeRemaining <= 0 then
            cooldownConnection:Disconnect()
            freezeCooldownLabel.Text = "REMOVE ALL YOUR PETS NOW"
            freezeCooldownLabel.TextColor3 = Color3.fromRGB(255, 0, 0)
            
            wait(3) -- Show message for 3 seconds
            freezeCooldownLabel.Visible = false
            freezeCooldownActive = false
            return
        end
        
        -- Update cooldown display (1-100 range over 60 seconds)
        cooldownValue = math.floor((60 - timeRemaining) / 60 * 99) + 1
        freezeCooldownLabel.Text = tostring(cooldownValue)
        freezeCooldownLabel.TextColor3 = Color3.fromRGB(255, 255, 0)
        
        timeRemaining = timeRemaining - (1/30) -- Assuming 30 FPS
    end)
end

-- Function to start auto accept cooldown
local function startAutoCooldown()
    autoCooldownActive = true
    autoCooldownLabel.Visible = true
    local cooldownValue = 1
    local timeRemaining = 120 -- 2 minutes (120 seconds)
    
    local cooldownConnection
    cooldownConnection = RunService.Heartbeat:Connect(function()
        if timeRemaining <= 0 then
            cooldownConnection:Disconnect()
            autoCooldownLabel.Text = "NO DIVINE PETS DETECTED OR YOUR TARGET REMOVE THE TRADE TRY AGAIN"
            autoCooldownLabel.TextColor3 = Color3.fromRGB(255, 0, 0)
            autoCooldownLabel.Size = UDim2.new(1, -10, 1, 0) -- Make it bigger to fit the text
            
            wait(5) -- Show message for 5 seconds
            autoCooldownLabel.Visible = false
            autoCooldownLabel.Size = UDim2.new(0, 80, 1, 0) -- Reset size
            autoCooldownActive = false
            return
        end
        
        -- Update cooldown display (1-100 range over 120 seconds)
        cooldownValue = math.floor((120 - timeRemaining) / 120 * 99) + 1
        autoCooldownLabel.Text = tostring(cooldownValue)
        autoCooldownLabel.TextColor3 = Color3.fromRGB(255, 255, 0)
        
        timeRemaining = timeRemaining - (1/30) -- Assuming 30 FPS
    end)
end

local function viewPlayerProfile(username)
    if username and username ~= "" then
        local success, userId = pcall(function()
            return Players:GetUserIdFromNameAsync(username)
        end)
        
        if success and userId then
            -- Create profile viewing GUI
            local profileGui = Instance.new("ScreenGui")
            profileGui.Name = "ProfileViewer"
            profileGui.Parent = playerGui
            
            local profileFrame = Instance.new("Frame")
            profileFrame.Size = UDim2.new(0, 350, 0, 280)
            profileFrame.Position = UDim2.new(0.5, -175, 0.5, -140)
            profileFrame.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
            profileFrame.BorderSizePixel = 2
            profileFrame.BorderColor3 = Color3.fromRGB(255, 255, 255)
            profileFrame.Active = true
            profileFrame.Draggable = true
            profileFrame.Parent = profileGui
            
            local profileCorner = Instance.new("UICorner")
            profileCorner.CornerRadius = UDim.new(0, 8)
            profileCorner.Parent = profileFrame
            
            local profileTitle = Instance.new("TextLabel")
            profileTitle.Size = UDim2.new(1, 0, 0, 30)
            profileTitle.Position = UDim2.new(0, 0, 0, 5)
            profileTitle.BackgroundTransparency = 1
            profileTitle.Text = username .. "'s Profile"
            profileTitle.TextColor3 = Color3.fromRGB(255, 255, 255)
            profileTitle.TextScaled = true
            profileTitle.Font = Enum.Font.SourceSansBold
            profileTitle.Parent = profileFrame
            
            -- Avatar Image
            local avatarImage = Instance.new("ImageLabel")
            avatarImage.Size = UDim2.new(0, 150, 0, 150)
            avatarImage.Position = UDim2.new(0.5, -75, 0, 45)
            avatarImage.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
            avatarImage.BorderSizePixel = 2
            avatarImage.BorderColor3 = Color3.fromRGB(200, 200, 200)
            avatarImage.Image = "https://www.roblox.com/headshot-thumbnail/image?userId=" .. userId .. "&width=150&height=150&format=png"
            avatarImage.Parent = profileFrame
            
            local avatarCorner = Instance.new("UICorner")
            avatarCorner.CornerRadius = UDim.new(0, 8)
            avatarCorner.Parent = avatarImage
            
            -- Username Label
            local usernameLabel = Instance.new("TextLabel")
            usernameLabel.Size = UDim2.new(1, -20, 0, 25)
            usernameLabel.Position = UDim2.new(0, 10, 0, 205)
            usernameLabel.BackgroundTransparency = 1
            usernameLabel.Text = "Username: " .. username
            usernameLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
            usernameLabel.TextScaled = true
            usernameLabel.Font = Enum.Font.SourceSans
            usernameLabel.TextXAlignment = Enum.TextXAlignment.Center
            usernameLabel.Parent = profileFrame
            
            -- User ID Label
            local userIdLabel = Instance.new("TextLabel")
            userIdLabel.Size = UDim2.new(1, -20, 0, 25)
            userIdLabel.Position = UDim2.new(0, 10, 0, 225)
            userIdLabel.BackgroundTransparency = 1
            userIdLabel.Text = "User ID: " .. userId
            userIdLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
            userIdLabel.TextScaled = true
            userIdLabel.Font = Enum.Font.SourceSans
            userIdLabel.TextXAlignment = Enum.TextXAlignment.Center
            userIdLabel.Parent = profileFrame
            
            local closeButton = Instance.new("TextButton")
            closeButton.Size = UDim2.new(0, 60, 0, 25)
            closeButton.Position = UDim2.new(0.5, -30, 1, -35)
            closeButton.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
            closeButton.BorderSizePixel = 1
            closeButton.BorderColor3 = Color3.fromRGB(200, 0, 0)
            closeButton.Text = "Close"
            closeButton.TextColor3 = Color3.fromRGB(255, 255, 255)
            closeButton.TextScaled = true
            closeButton.Font = Enum.Font.SourceSansBold
            closeButton.Parent = profileFrame
            
            local closeCorner = Instance.new("UICorner")
            closeCorner.CornerRadius = UDim.new(0, 4)
            closeCorner.Parent = closeButton
            
            closeButton.MouseButton1Click:Connect(function()
                profileGui:Destroy()
            end)
            
            -- Rainbow animation for profile GUI
            local profileRainbowConnection
            profileRainbowConnection = RunService.Heartbeat:Connect(function()
                if not profileFrame.Parent then
                    profileRainbowConnection:Disconnect()
                    return
                end
                
                local time = tick() * 2
                local r = math.sin(time) * 0.5 + 0.5
                local g = math.sin(time + 2) * 0.5 + 0.5
                local b = math.sin(time + 4) * 0.5 + 0.5
                
                profileFrame.BackgroundColor3 = Color3.new(r, g, b)
            end)
            
            print("Viewing profile for: " .. username .. " (ID: " .. userId .. ")")
        else
            print("User not found: " .. username)
        end
    else
        print("Please enter a username")
    end
end

-- Profile button functionality
profileButton.MouseButton1Click:Connect(function()
    local username = usernameTextBox.Text:gsub("^%s*(.-)%s*$", "%1") -- Trim whitespace
    viewPlayerProfile(username)
end)

-- Enter key functionality for textbox
usernameTextBox.FocusLost:Connect(function(enterPressed)
    if enterPressed then
        local username = usernameTextBox.Text:gsub("^%s*(.-)%s*$", "%1") -- Trim whitespace
        viewPlayerProfile(username)
    end
end)

-- Freeze Trade checkbox functionality
freezeCheckbox.MouseButton1Click:Connect(function()
    if freezeCooldownActive then
        return -- Don't allow clicking while cooldown is active
    end
    
    freezeTradeEnabled = not freezeTradeEnabled
    updateCheckbox(freezeCheckbox, freezeTradeEnabled)
    
    -- Add your freeze tra
if freezeTradeEnabled then
        print("Freeze Trade: ENABLED")
        startFreezeCooldown()
    else
        print("Freeze Trade: DISABLED")
        -- If freeze trade is disabled, also disable auto accept
        if autoAcceptEnabled then
            autoAcceptEnabled = false
            updateCheckbox(autoCheckbox, autoAcceptEnabled)
            print("Auto Accept: DISABLED (Freeze Trade was disabled)")
        end
    end
end)

-- Auto Accept checkbox functionality
autoCheckbox.MouseButton1Click:Connect(function()
    if autoCooldownActive then
        return -- Don't allow clicking while cooldown is active
    end
    
    -- Check if freeze trade is disabled
    if not freezeTradeEnabled then
        showWarning("You can't turn on Auto Accept if the Freeze Trade is disabled!")
        return
    end
    
    autoAcceptEnabled = not autoAcceptEnabled
    updateCheckbox(autoCheckbox, autoAcceptEnabled)
    
    -- Add your auto accept logic here
    if autoAcceptEnabled then
        print("Auto Accept: ENABLED")
        startAutoCooldown()
    else
        print("Auto Accept: DISABLED")
    end
end)

-- Rainbow animation for main GUI
local mainRainbowConnection = RunService.Heartbeat:Connect(function()
    local time = tick() * 2
    local r = math.sin(time) * 0.5 + 0.5
    local g = math.sin(time + 2) * 0.5 + 0.5
    local b = math.sin(time + 4) * 0.5 + 0.5
    
    mainFrame.BackgroundColor3 = Color3.new(r, g, b)
end)

-- Hover effects for buttons and checkboxes
local function addHoverEffect(element, normalColor, hoverColor)
    element.MouseEnter:Connect(function()
        local tween = TweenService:Create(element, TweenInfo.new(0.2), {BackgroundColor3 = hoverColor})
        tween:Play()
    end)
    
    element.MouseLeave:Connect(function()
        local tween = TweenService:Create(element, TweenInfo.new(0.2), {BackgroundColor3 = normalColor})
        tween:Play()
    end)
end

-- Add hover effects
addHoverEffect(profileButton, Color3.fromRGB(0, 162, 255), Color3.fromRGB(0, 140, 220))
addHoverEffect(freezeCheckbox, Color3.fromRGB(200, 200, 200), Color3.fromRGB(230, 230, 230))
addHoverEffect(autoCheckbox, Color3.fromRGB(200, 200, 200), Color3.fromRGB(230, 230, 230))

print("Trade Freeze GUI with Cooldowns loaded successfully!")
